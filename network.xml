<?xml version="1.0"?>
<tweaks>
	<tweak name="settings/network" extension="network_settings">
		<search>
			<network/>
			<rpc/>
			<message name="lobby_info"/>
			<param type="int"/>
		</search>
		<target mode="attributes" multiple="true">
			<attr name="max" value="1000"/>
		</target>
	</tweak>
	
	<tweak name="settings/network" extension="network_settings">
		<search>
			<network/>
			<rpc/>
			<message name="sync_profile"/>
			<param type="int"/>
		</search>
		<target mode="attributes" multiple="true">
			<attr name="max" value="1000"/>
		</target>
	</tweak>
</tweaks>
